{
  "expo": {
    "name": "chatboat",
    "slug": "chatboat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chatboat.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chatboat.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yayanfr20.chatboat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/chatboat.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a47c9a41-7aa0-437f-8032-392b99ea3a4c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a47c9a41-7aa0-437f-8032-392b99ea3a4c"
    }
  }
}
